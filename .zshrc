# Example aliases
# alias zshconfig="mate ~/.zshrc"
# alias ohmyzsh="mate ~/.oh-my-zsh"
alias gs="git status"
alias ga="git add ."
alias gc="git checkout"
alias gco="git commit -m"
alias gp="git pull"

